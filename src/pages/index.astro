---
import Layout from '~/layouts/PageLayout.astro';
import WidgetWrapper from '~/components/ui/WidgetWrapper.astro';
import Process from '~/components/react/Tailwind/Process';
// import Hero from '~/components/widgets/Hero.astro';
import { FadeIn } from '~/components/react/Animations/FadeIn';
import Header from '~/components/widgets/Header.astro';
import Image from '~/components/common/Image.astro';
import { headerData } from '~/navigation';
import BlogLatestPosts from '~/components/widgets/BlogLatestPosts.astro';
import Stats from '~/components/widgets/Stats.astro';
// import CallToAction from '~/components/widgets/CallToAction.astro';
import Technoloiges from '~/components/react/Tailwind/Technologies';
import Content from '~/components/react/Tailwind/Content';
import TailwindTestimonials from '~/components/react/Tailwind/Testimonials';
import TallyForm from '~/components/widgets/TallyForm.astro';
import SimpleCalendly from '~/components/widgets/SimpleCalendly.astro';

const metadata = {
  title: 'venger.me - Your catalyst for unstoppable growth',
  ignoreTitleTemplate: true,
};
---

<script type="module">
  document.querySelectorAll('a[href^="#"]').forEach((anchor) => {
    anchor.addEventListener('click', function (e) {
      e.preventDefault();
      const hashVal = this.getAttribute('href');
      const targetAnchor = document.querySelectorAll(hashVal)[0];
      const contactPos = window.pageYOffset + targetAnchor.getBoundingClientRect().top;
      window.scrollTo({ top: contactPos, behavior: 'smooth' });
    });
  });
</script>
<Layout metadata={metadata}>
  <!-- Hero Widget ******************* -->
  <Fragment slot="header">
    <Header {...headerData} showToggleTheme />

    <div class="bg-white">
      <div class="relative isolate overflow-hidden bg-gradient-to-b from-pink-100/30 pt-14">
        <div
          class="absolute inset-y-0 right-1/2 -z-10 -mr-96 w-[200%] origin-top-right skew-x-[-30deg] bg-white shadow-xl shadow-pink-600/10 ring-1 ring-pink-50 sm:-mr-80 lg:-mr-96"
          aria-hidden="true"
        >
        </div>
        <div class="mx-auto sm:py-40 lg:px-8 relative max-w-6xl px-4 md:px-6 py-12 md:py-16 lg:py-12 text-default">
          <div
            class="mx-auto max-w-2xl lg:mx-0 lg:grid lg:max-w-none lg:grid-cols-2 lg:gap-x-16 lg:gap-y-6 xl:grid-cols-1 xl:grid-rows-1 xl:gap-x-8"
          >
            <FadeIn client:load>
              <h1
                class="max-w-2xl text-4xl font-bold tracking-tight text-gray-900 sm:text-6xl lg:col-span-2 xl:col-auto"
              >
                Marketing funnels, automation and reporting for your business
              </h1>
            </FadeIn>
            <div class="mt-6 max-w-xl lg:mt-0 xl:col-end-1 xl:row-start-1">
              <p class="text-lg leading-8 text-gray-600">
                I tackle complex marketing challenges other marketers can't solve. Proved by more than 50 satisfied
                clients. Read about what I do or let's get in touch right away.
              </p>
              <div class="mt-10 flex items-center gap-x-6">
                <a
                  href="#contact"
                  class="rounded-md bg-primary px-3.5 py-2.5 text-sm font-semibold text-white shadow-sm hover:bg-primary focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600"
                  >Get in touch</a
                >
                <a href="#process" class="text-sm font-semibold leading-6 text-gray-900"
                  >Read more <span aria-hidden="true">â†’</span></a
                >
              </div>
            </div>
            <Image
              src="~/assets/images/venger.jpeg"
              alt=""
              class="mt-10 aspect-[6/5] w-full max-w-lg rounded-2xl object-cover sm:mt-16 lg:mt-0 lg:max-w-none xl:row-span-2 xl:row-end-2 xl:mt-36"
            />
          </div>
        </div>
        <div class="absolute inset-x-0 bottom-0 -z-10 h-24 bg-gradient-to-t from-white sm:h-32"></div>
      </div>
    </div>

